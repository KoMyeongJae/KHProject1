CREATE TABLE LIKE_REFER(
    LIKE_ID NUMBER(12) PRIMARY KEY,
    REFLIKE_SEQ NUMBER(8) NOT NULL,
    ID VARCHAR2(50) NOT NULL
);

SELECT * FROM LIKE_REFER;

DROP TABLE LIKE_REFER
CASCADE CONSTRAINTS;

ALTER TABLE LIKE_REFER
ADD CONSTRAINT FK_REFLIKE_SEQ FOREIGN KEY(REFLIKE_SEQ)
REFERENCES REFERROOM(SEQ);

ALTER TABLE LIKE_REFER
ADD CONSTRAINT FK_REFLIKE_ID FOREIGN KEY(ID)
REFERENCES MEMBER(ID);